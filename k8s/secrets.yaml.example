# Example secrets file - DO NOT commit actual secrets to git
# Create actual secrets using: kubectl create secret generic geoqb-secrets --from-literal=...

apiVersion: v1
kind: Secret
metadata:
  name: geoqb-secrets
  namespace: default
type: Opaque
stringData:
  database-url: "postgresql://user:password@host:5432/geoqb"
  redis-url: "redis://host:6379/0"
  secret-key: "your-super-secret-key-change-in-production"
  tigergraph-username: "tigergraph"
  tigergraph-password: "your-tigergraph-password"
  sendgrid-api-key: "your-sendgrid-api-key"
  stripe-secret-key: "sk_test_..."
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: geoqb-config
  namespace: default
data:
  tigergraph-host: "your-tigergraph-host.com"
  environment: "production"
